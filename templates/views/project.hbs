<!-- Header Item -->
{{#each projectitem}}
    {{#if @first}}
        {{> project_header this}}
    {{/if}}
{{/each}}
<!-- Project Navbar -->
{{> project_navbar projectitem}}
<!-- Remaining Items -->
{{#each projectitem}}
    {{#if @first}}{{else}}
        {{> project_row this}}
    {{/if}}
{{/each}}
